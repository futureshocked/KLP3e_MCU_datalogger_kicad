
(rules PCB MCU Datalogger 4-layer
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (eu.mihosoft.freerouting.autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 869)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.6)
      (against_preferred_direction_trace_costs 3.1)
    )
    (layer_rule In1.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
    (layer_rule In2.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.5)
    )
    (layer_rule B.Cu
      (active off)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.6)
      (against_preferred_direction_trace_costs 3.1)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 250.2 (type default_Power))
    (clear 50.0 (type smd_smd))
    (clear 250.2 (type smd_Power))
    (clear 250.2 (type "kicad_default"_Power))
    (clear 250.2 (type Power_Power))
  )
  (padstack "Via[0-3]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle In1.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle In2.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-3]_900:400_um"
    (shape
      (circle F.Cu 900.0 0.0 0.0)
    )
    (shape
      (circle In1.Cu 900.0 0.0 0.0)
    )
    (shape
      (circle In2.Cu 900.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 900.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-3]_800:400_um" "Via[0-3]_800:400_um" default
  )
  (via 
    "Via[0-3]_900:400_um" "Via[0-3]_900:400_um" default
  )
  (via 
    "Via[0-3]_800:400_um-kicad_default" "Via[0-3]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-3]_900:400_um-kicad_default" "Via[0-3]_900:400_um" "kicad_default"
  )
  (via 
    "Via[0-3]_800:400_um-Power" "Via[0-3]_800:400_um" Power
  )
  (via 
    "Via[0-3]_900:400_um-Power" "Via[0-3]_900:400_um" Power
  )
  (via_rule
    default "Via[0-3]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-3]_800:400_um-kicad_default"
  )
  (via_rule
    Power "Via[0-3]_900:400_um-Power"
  )
  (class default
    "Net-(C2-Pad2)" "Net-(C3-Pad2)" /Abc123 "Net-(D1-Pad1)" /SCK "Net-(D2-Pad1)" /SDA /D2
    /D3 /D4 /D5 /D6 /D7 /D8 /RX /TX
    /MISO /MOSI /RESET "Net-(R1-Pad2)" "Net-(R2-Pad2)" "Net-(U2-Pad1)" "Net-(U2-Pad2)"
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu In1.Cu In2.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu In1.Cu In2.Cu)
    )
  )
  (class Power
    GND /Vcc
    (clearance_class Power)
    (via_rule Power)
    (rule
      (width 350.0)
    )
    (circuit 
      (use_layer F.Cu In1.Cu In2.Cu)
    )
  )
)